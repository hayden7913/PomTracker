{
  "name": "new-fullstack-boilerplate",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebookincubator/create-react-app).",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hayden7913/new-fullstack-boilerplate.git"
  },
  "devDependencies": {
    "concurrently": "^3.4.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "body-parser": "^1.17.2",
    "concurrently": "^3.4.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "mongodb": "^2.2.28",
    "mongoose": "^4.10.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "scripts": {
    "scripts": {
      "start": "concurrently \"node server.js\"  \"cd client && npm start\"",
      "server": "babel-node server.js",
      "test": "mocha",
      "lint": "eslint --ext .js --ext .jsx client/src/components/side-bar/ || exit 0",
      "lint:fix": "eslint --fix --ext .js --ext .jsx client/src/components/ || exit 0",
      "build:client": "cd client && npm run build",
      "push:git": "git add . && git commit -m \"build\" && git push origin master || exit 0",
      "push:heroku": "git push heroku master",
      "deploy": "npm run build:client && npm run push:git && npm run push:heroku"
    }
  }
}
